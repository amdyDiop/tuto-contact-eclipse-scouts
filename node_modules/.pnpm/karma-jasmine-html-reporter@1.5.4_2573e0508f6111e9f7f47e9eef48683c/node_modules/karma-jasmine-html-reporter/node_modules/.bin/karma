#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/karma@5.2.2/node_modules/karma/bin/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/karma@5.2.2/node_modules/karma/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/karma@5.2.2/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules:/home/amdy/Documents/codes/eclipse scout/node_modules:/home/amdy/Documents/codes/node_modules:/home/amdy/Documents/node_modules:/home/amdy/node_modules:/home/node_modules:/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/karma-jasmine-html-reporter@1.5.4_2573e0508f6111e9f7f47e9eef48683c/node_modules/karma-jasmine-html-reporter/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/karma-jasmine-html-reporter@1.5.4_2573e0508f6111e9f7f47e9eef48683c/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../karma@5.2.2/node_modules/karma/bin/karma" "$@"
else
  exec node  "$basedir/../../../../../karma@5.2.2/node_modules/karma/bin/karma" "$@"
fi
