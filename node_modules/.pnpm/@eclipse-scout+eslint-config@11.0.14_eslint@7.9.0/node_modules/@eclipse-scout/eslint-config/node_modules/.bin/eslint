#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/eslint@7.9.0/node_modules/eslint/bin/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/eslint@7.9.0/node_modules/eslint/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/eslint@7.9.0/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules:/home/amdy/Documents/codes/eclipse scout/node_modules:/home/amdy/Documents/codes/node_modules:/home/amdy/Documents/node_modules:/home/amdy/node_modules:/home/node_modules:/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/@eclipse-scout+eslint-config@11.0.14_eslint@7.9.0/node_modules/@eclipse-scout/eslint-config/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/@eclipse-scout+eslint-config@11.0.14_eslint@7.9.0/node_modules/@eclipse-scout/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/@eclipse-scout+eslint-config@11.0.14_eslint@7.9.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../eslint@7.9.0/node_modules/eslint/bin/eslint.js" "$@"
else
  exec node  "$basedir/../../../../../../eslint@7.9.0/node_modules/eslint/bin/eslint.js" "$@"
fi
