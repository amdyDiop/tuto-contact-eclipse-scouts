#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/js-yaml@3.14.1/node_modules/js-yaml/bin/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/js-yaml@3.14.1/node_modules/js-yaml/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/js-yaml@3.14.1/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules:/home/amdy/Documents/codes/eclipse scout/node_modules:/home/amdy/Documents/codes/node_modules:/home/amdy/Documents/node_modules:/home/amdy/node_modules:/home/node_modules:/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/write-yaml-file@4.0.0/node_modules/write-yaml-file/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/write-yaml-file@4.0.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../js-yaml@3.14.1/node_modules/js-yaml/bin/js-yaml.js" "$@"
else
  exec node  "$basedir/../../../../../js-yaml@3.14.1/node_modules/js-yaml/bin/js-yaml.js" "$@"
fi
