#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/webpack@4.44.1/node_modules/webpack/bin/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/webpack@4.44.1/node_modules/webpack/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/webpack@4.44.1/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules:/home/amdy/Documents/codes/eclipse scout/node_modules:/home/amdy/Documents/codes/node_modules:/home/amdy/Documents/node_modules:/home/amdy/node_modules:/home/node_modules:/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/optimize-css-assets-webpack-plugin@5.0.4_webpack@4.44.1/node_modules/optimize-css-assets-webpack-plugin/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/optimize-css-assets-webpack-plugin@5.0.4_webpack@4.44.1/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../webpack@4.44.1/node_modules/webpack/bin/webpack.js" "$@"
else
  exec node  "$basedir/../../../../../webpack@4.44.1/node_modules/webpack/bin/webpack.js" "$@"
fi
