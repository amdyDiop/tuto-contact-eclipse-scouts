#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/which@1.3.1/node_modules/which/bin/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/which@1.3.1/node_modules/which/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/which@1.3.1/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules:/home/amdy/Documents/codes/eclipse scout/node_modules:/home/amdy/Documents/codes/node_modules:/home/amdy/Documents/node_modules:/home/amdy/node_modules:/home/node_modules:/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/karma-chrome-launcher@3.1.0/node_modules/karma-chrome-launcher/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/karma-chrome-launcher@3.1.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../which@1.3.1/node_modules/which/bin/which" "$@"
else
  exec node  "$basedir/../../../../../which@1.3.1/node_modules/which/bin/which" "$@"
fi
