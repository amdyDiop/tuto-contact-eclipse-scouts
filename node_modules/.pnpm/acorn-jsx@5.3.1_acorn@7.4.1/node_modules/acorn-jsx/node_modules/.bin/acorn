#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/acorn@7.4.1/node_modules/acorn/bin/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/acorn@7.4.1/node_modules/acorn/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/acorn@7.4.1/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules:/home/amdy/Documents/codes/eclipse scout/node_modules:/home/amdy/Documents/codes/node_modules:/home/amdy/Documents/node_modules:/home/amdy/node_modules:/home/node_modules:/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/acorn-jsx@5.3.1_acorn@7.4.1/node_modules/acorn-jsx/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/acorn-jsx@5.3.1_acorn@7.4.1/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../acorn@7.4.1/node_modules/acorn/bin/acorn" "$@"
else
  exec node  "$basedir/../../../../../acorn@7.4.1/node_modules/acorn/bin/acorn" "$@"
fi
