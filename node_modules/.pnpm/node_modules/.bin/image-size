#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/image-size@0.5.5/node_modules/image-size/bin/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/image-size@0.5.5/node_modules/image-size/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/image-size@0.5.5/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules:/home/amdy/Documents/codes/eclipse scout/node_modules:/home/amdy/Documents/codes/node_modules:/home/amdy/Documents/node_modules:/home/amdy/node_modules:/home/node_modules:/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/node_modules/image-size/bin/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/node_modules/image-size/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../image-size/bin/image-size.js" "$@"
else
  exec node  "$basedir/../image-size/bin/image-size.js" "$@"
fi
