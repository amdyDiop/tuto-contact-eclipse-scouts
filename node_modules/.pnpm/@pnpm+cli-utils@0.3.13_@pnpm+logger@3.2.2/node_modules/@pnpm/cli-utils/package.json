{
	"name": "@pnpm/cli-utils",
	"version": "0.3.13",
	"description": "Utils for pnpm commands",
	"main": "lib/index.js",
	"typings": "lib/index.d.ts",
	"files": [
		"lib",
		"!*.map"
	],
	"engines": {
		"node": ">=10.13"
	},
	"scripts": {
		"lint": "tslint -c ../../tslint.json src/**/*.ts test/**/*.ts",
		"pretest": "rimraf node_modules/.bin/pnpm",
		"_test": "cd ../.. && c8 --reporter lcov --reports-dir packages/cli-utils/coverage ts-node packages/cli-utils/test --type-check",
		"test": "pnpm run compile && pnpm run _test",
		"prepublishOnly": "pnpm run compile",
		"compile": "rimraf lib tsconfig.tsbuildinfo && tsc --build"
	},
	"repository": "https://github.com/pnpm/pnpm/blob/master/packages/cli-utils",
	"keywords": [
		"pnpm"
	],
	"author": "Zoltan Kochan <z@kochan.io> (https://www.kochan.io/)",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/pnpm/pnpm/issues"
	},
	"homepage": "https://github.com/pnpm/pnpm/blob/master/packages/cli-utils#readme",
	"devDependencies": {
		"@pnpm/cli-utils": "link:",
		"@pnpm/logger": "^3.1.0",
		"@pnpm/types": "5.0.0",
		"@types/nopt": "^3.0.29",
		"@types/ramda": "^0.27.0"
	},
	"dependencies": {
		"@pnpm/command": "1.0.0",
		"@pnpm/config": "8.1.2",
		"@pnpm/error": "1.2.0",
		"@pnpm/package-is-installable": "4.0.6",
		"@pnpm/read-project-manifest": "1.0.3",
		"@pnpm/utils": "0.13.0",
		"chalk": "3.0.0",
		"load-json-file": "6.2.0",
		"nopt": "4.0.3",
		"ramda": "0.27.0",
		"version-selector-type": "2.0.1"
	},
	"peerDependencies": {
		"@pnpm/logger": "^3.1.0"
	}
}
