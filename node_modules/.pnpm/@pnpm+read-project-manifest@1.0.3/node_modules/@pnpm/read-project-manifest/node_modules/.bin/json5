#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/json5@2.1.2/node_modules/json5/lib/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/json5@2.1.2/node_modules/json5/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/json5@2.1.2/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules:/home/amdy/Documents/codes/eclipse scout/node_modules:/home/amdy/Documents/codes/node_modules:/home/amdy/Documents/node_modules:/home/amdy/node_modules:/home/node_modules:/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/@pnpm+read-project-manifest@1.0.3/node_modules/@pnpm/read-project-manifest/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/@pnpm+read-project-manifest@1.0.3/node_modules/@pnpm/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/@pnpm+read-project-manifest@1.0.3/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../json5@2.1.2/node_modules/json5/lib/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../json5@2.1.2/node_modules/json5/lib/cli.js" "$@"
fi
