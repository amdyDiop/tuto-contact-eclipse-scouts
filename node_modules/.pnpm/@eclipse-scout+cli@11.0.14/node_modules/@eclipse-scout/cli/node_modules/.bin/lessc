#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/less@3.12.2/node_modules/less/bin/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/less@3.12.2/node_modules/less/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/less@3.12.2/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules:/home/amdy/Documents/codes/eclipse scout/node_modules:/home/amdy/Documents/codes/node_modules:/home/amdy/Documents/node_modules:/home/amdy/node_modules:/home/node_modules:/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/@eclipse-scout+cli@11.0.14/node_modules/@eclipse-scout/cli/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/@eclipse-scout+cli@11.0.14/node_modules/@eclipse-scout/node_modules:/home/amdy/Documents/codes/eclipse scout/contacts/node_modules/.pnpm/@eclipse-scout+cli@11.0.14/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../less@3.12.2/node_modules/less/bin/lessc" "$@"
else
  exec node  "$basedir/../../../../../../less@3.12.2/node_modules/less/bin/lessc" "$@"
fi
